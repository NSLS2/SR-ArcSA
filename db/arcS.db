record(seq, 	 "$(S1){$(D1)}Cmd:Ini") {
  field(PINI,    "YES")
  field(DO1,     "11")
  field(LNK1,    "$(S1){$(D1)}Cmd:Update.SCAN")
  field(DO2,     "1")
  field(LNK2,    "$(S1){$(D1)}Cmd:Update PP")
  field(DO2,     "3.0")
}

record(bo,       "$(S1){$(D1)}Cmd:Update") {
  field(DTYP,    "asynInt32")
  field(OUT ,    "@asyn($(PORT),0)Update")
  field(ZNAM,    "Update0")
  field(ONAM,    "Update1")
  field(IVOA,    "Don't drive outputs")
  field(FLNK,    "$(S1){$(D1)}DI:Ep20-I")
  info(autosaveFields_pass0, "SCAN")
}

